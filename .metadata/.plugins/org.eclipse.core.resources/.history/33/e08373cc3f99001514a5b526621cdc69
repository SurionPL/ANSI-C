#include <avr/io.h>
#include <util/delay.h>


void initializeTimers()
{
	//Dla 328P
	TCCR1B = (1<<WGM12) |
			 (1<<CS12);  //Preskaler 1024    7372800/1024=7200
	OCR1A = 7200;
	TIMSK1 = 1 << OCIE1A;   //Aktywowanie przerwan CTC OCR1A



	//Dla 328P
//	TCCR1B = (1<<WGM12) |
//			 (1<<CS12);  //Preskaler 1024    7372800/1024=7200
//	OCR1A = 7200;
//	TIMSK1 = 1 << OCIE1A;   //Aktywowanie przerwan CTC OCR1A
}



int main()
{
	DDRB = 1<< PB1;
	PORTB = 2;
	while(1) {
		PORTB ^= 1<<PB1;
		_delay_ms(2000);
	}
}
