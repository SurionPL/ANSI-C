#include <avr/io.h>
#include <util/delay.h>
#include <avr/interrupt.h>

#include "MKUART/mkuart.h"
#include "BH1750.h"

int main()
{
	USART_Init(__UBRR);
	sei();
	BH1750_PowerOn();

	//BH1750_Read();
	while(1)
	{
		BH1750_Start(BH1750_OTLR_MODE;

	}
}
