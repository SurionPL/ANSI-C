#include <avr/io.h>
#include <util/delay.h>
#include "RTC.h"
#include "I2C.h"

int8_t temp1;
uint8_t temp2;


int main() {
	TWI_Init(100000/100);

	RTC_InitTypeDef RTC_Init;
	RTC_Init.BB_SQW = DISABLE;
	RTC_Init.RTC_Alarm1=Alarm1_Disable;
	RTC_Init.RTC_Alarm2=Alarm2_Disable;
	RTC_Init.RTC_Oscillator = DISABLE;
	RTC_Init.RTC_SQW = SQW_Freq_1Hz;

	RTC_DateTimeTypedef DataCzas;
	DataCzas.seconds = 0;
	DataCzas.minutes = 40;
	DataCzas.hours = 23;

	RTC_GetTemp(&temp1, &temp2);

while(1){} 
}
