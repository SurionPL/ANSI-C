#ifndef MAIN_H_
#define MAIN_H_


#define MEASUREMENTS_DISABLE ((uint8_t)0x00)	/* Brak zezwolenia na pomiary */
#define MEASUREMENTS_ENABLE  ((uint8_t)0x01)	/* Zezwolenie na pomiary */

#define MEASUREMENTS_TRIGGER_DISABLE ((uint8_t)0x00)	/* Brak zgody na wyzwolenie pomiarow */
#define MEASUREMENTS_TRIGGER_ENABLE  ((uint8_t)0x01)	/* Zgodna na wyzwolenie pomiarow */




#endif /* MAIN_H_ */
